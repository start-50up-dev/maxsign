<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealWorldApp.MainPage">

    <StackLayout>
        <Entry Placeholder="Enter User Name" x:Name="EntUserName" />
        <Button Text="Save" x:Name="BtnSave" Clicked="BtnSave_Clicked" />
        <Button Text="Retrieve" x:Name="BtnRetrieve" Clicked="BtnRetrieve_Clicked" />
        <Label x:Name="LblUserName" />
    </StackLayout>

</ContentPage>
